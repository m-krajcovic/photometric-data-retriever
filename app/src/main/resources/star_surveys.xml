<list>
    <starSurvey name="NSVS">
        <plugin name="NSVS"/>
        <patterns>
            <java.util.regex.Pattern>
                <pattern>NSVS\s(?&lt;id&gt;\d*)</pattern>
                <flags>0</flags>
            </java.util.regex.Pattern>
        </patterns>
        <valueParams/>
        <urls>
            <string>http://skydot.lanl.gov/nsvs/star.php?num={id}&amp;mask=32004</string>
        </urls>
    </starSurvey>
    <starSurvey name="CRTS">
        <plugin name="CRTS"/>
        <patterns/>
        <valueParams/>
        <urls>
            <string>http://nunuku.caltech.edu/cgi-bin/getcssconedb_release_img.cgi?RA={ra}&amp;Dec={dec}&amp;Rad=0.2&amp;DB=photcat&amp;OUT=csv&amp;SHORT=short&amp;PLOT=plot</string>
        </urls>
    </starSurvey>
    <starSurvey name="SWASP">
        <plugin/>
        <patterns>
            <java.util.regex.Pattern>
                <pattern>(?&lt;obj&gt;1SWASP\s(?&lt;id&gt;J.*))</pattern>
                <flags>0</flags>
            </java.util.regex.Pattern>
        </patterns>
        <valueParams/>
        <urls>
            <string>http://wasp.cerit-sc.cz/json?type=CSV&amp;object={obj}</string>
            <string>http://wasp.cerit-sc.cz/search?ra={ra}&amp;dec={dec}&amp;radius=1&amp;radiusUnit=min&amp;limit=1</string>
        </urls>
    </starSurvey>
</list>
