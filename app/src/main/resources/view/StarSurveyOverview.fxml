<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65"
            fx:controller="cz.muni.physics.pdr.app.controller.StarSurveyOverviewController">
    <children>
        <TableView fx:id="starSurveys" editable="true" maxHeight="-Infinity" maxWidth="600" minHeight="-Infinity"
                   minWidth="600" prefHeight="309.0" prefWidth="600.0" AnchorPane.bottomAnchor="40.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="68.0" text="Name"/>
                <TableColumn fx:id="sesameIdentifierColumn" prefWidth="163.0" text="Sesame Identifier"/>
                <TableColumn fx:id="urlColumn" prefWidth="258.0" text="URL"/>
                <TableColumn fx:id="pluginColumn" minWidth="0.0" prefWidth="110.0" text="Plugin"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <ButtonBar layoutX="336.0" layoutY="310.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.rightAnchor="14.0">
            <buttons>
                <Button fx:id="newButton" mnemonicParsing="false" onAction="#handleNewButton" text="New..."/>
                <Button fx:id="editButton" mnemonicParsing="false" onAction="#handleEditButton" text="Edit..."/>
                <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#handleDeleteButton" text="Delete"/>
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
