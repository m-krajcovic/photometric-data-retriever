<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<TableView xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:id="stellarObjectsTable"
           layoutX="186.0" layoutY="48.0" prefHeight="200.0" prefWidth="200.0"
           fx:controller="cz.muni.physics.pdr.app.controller.StellarObjectTableController">
    <columns>
        <TableColumn fx:id="distanceColumn" prefWidth="75.0" text="%stellarobject.distance"/>
        <TableColumn fx:id="nameColumn" prefWidth="137.0" text="%stellarobject.name"/>
        <TableColumn fx:id="raColumn" minWidth="6.0" prefWidth="189.0" text="%stellarobject.ra"/>
        <TableColumn fx:id="decColumn" prefWidth="198.0" text="%stellarobject.dec"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>
